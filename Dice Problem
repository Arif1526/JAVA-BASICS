import java.util.*;
class Dice {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int num = sc.nextInt();
        int c=0;
        for(int i=0;i<=6;i++) {
            for(int j=0;j<=6;j++) {
                if(i+j==num) {
                    System.out.println(i+" "+j);
                   c+=1;
                }
            }
        }
        System.out.println("Total Probability: "+c);

    }
}
