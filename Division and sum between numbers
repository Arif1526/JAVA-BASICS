import java.util.*;
class Arif{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num1=sc.nextInt();
        int num2=sc.nextInt();
        System.out.println(num(num1,num2));

    }
    public static int num(int num1,int num2){
        int sum=0;
        for (int i = num1; i <=num2 ; i++) {
            if( i%3==0 && i%5==0 ){
                sum+=i;
            }
        }
        return sum;
    }
}
