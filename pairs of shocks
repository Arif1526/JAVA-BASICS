import java.util.Scanner;
class shocks
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] hash=new int[200];
        for (int i = 1; i <=n ; i++) {
            hash[sc.nextInt()]++;
        }
        int res=0;
        for(int p:hash){
            res+=p/2;
        }
        System.out.println(res);
    }
}
